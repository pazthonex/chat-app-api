{
  "family": "chat-api-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "chat-api-container",
      "image": "${IMAGE_URI}",
      "portMappings": [
        {
          "containerPort": 9000,
          "hostPort": 9000,
          "protocol": "tcp"
        }
      ],
      "environment": [],
      "secrets": [],
      "essential": true
    }
  ]
}
